<filetype binary="false" description="Cascading Configuration File" name="CCS">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="//" />
      <option name="COMMENT_START" value="/*" />
      <option name="COMMENT_END" value="*/" />
      <option name="HEX_PREFIX" value="0x" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="@override" ignore_case="false" />
    <keywords2 keywords="@constrain" />
    <keywords3 keywords="@context" />
    <keywords4 keywords="@import" />
  </highlighting>
  <extensionMap>
    <mapping ext="ccs" />
  </extensionMap>
</filetype>